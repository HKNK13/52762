    "\n",
    "def factorial(sayi):\n",
    "    faktoriyel=1\n",
    "    if(sayi==0 or sayi==1):\n",
    "        return 1 \n",
    "    while sayi>=1:\n",
    "        faktoriyel*=sayi\n",
    "        sayi-=1\n",
    "    return faktoriyel\n",
    "    \n",
    "factorial(5)\n"
   ]
  }
